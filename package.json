{
  "name": "phetscraper",
  "version": "2.2.2",
  "description": "Scraper and exporter of PHET simulations",
  "main": "index.js",
  "dependencies": {
    "@babel/core": "^7.20.5",
    "@openzim/libzim": "^2.4.4",
    "@types/cheerio": "^0.22.31",
    "@types/cli-progress": "^3.11.0",
