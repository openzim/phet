    "babel-register": "^6.26.0",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "cheerio": "^0.22.0",
    "cli-progress": "^3.11.2",
    "dotenv": "^8.6.0",
    "glob": "^7.2.3",
    "got": "^12.1.0",
    "handlebars": "^4.7.7",
    "hoek": "^6.1.3",
    "html-minifier": "^4.0.0",
    "imagemin": "^7.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegoptim": "^7.0.0",
    "imagemin-pngcrush": "^7.0.0",
