    "jest": "^26.6.3",
    "js-yaml": "^4.1.0",
    "md5": "^2.3.0",
    "ncp": "^2.0.0",
    "object-path": "^0.11.8",
    "rimraf": "^3.0.2",
    "slugify": "^1.6.5",
    "snyk": "^1.1064.0",
